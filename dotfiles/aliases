# Navigation
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."
alias ~="cd ~"
alias -- -="cd -"

# Shortcuts
alias d="cd ~/Downloads"
alias p="cd ~/Projects"
alias g="git"
alias h="history"
alias j="jobs"

colorflag="-G"

# List
alias ll="ls -lhF ${colorflag}"
alias la="ls -lhaF ${colorflag}"
